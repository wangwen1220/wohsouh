<body class="body_bg">

<div id="mian">
<div class="box">
  <div class="meili_nr">
  <div class="gq_music_daoyu">引语：
  	{db sql="SELECT * FROM #table_section s LEFT JOIN #table_section_data d ON d.sectionid=s.sectionid WHERE s.memo = 'festival_zhuanti_music_introduction' ORDER BY d.sort desc limit 1"}
  {$r[description]}
  {/db}
  </div>
   <div class="gq_music_left">
   <div class="gq_music_left_bt"><h1>MV欣赏</h1></div>
<div class="mtv_zt_video_nr">
			<ul class="mtv_zt_video_list">
				{db sql="SELECT * FROM #table_section s LEFT JOIN #table_section_data d ON d.sectionid=s.sectionid WHERE s.memo = 'festival_zhuanti_music_mv' and d.thumb!='' ORDER BY d.sort desc limit 14"}
				<li>
					<span class="gq_music_bg"><a target="_blank" href="{$r[url]}" title="{$r[title]}">
					<img src="{WWW_URL}upload/{$r[thumb]}" width="130" height="83" /></a></span>
					<span class="top_video_text">
					<a target="_blank" href="{$r[url]}" title="{$r[title]}">{str_cut($r[title],20,'')}</a>
					</span>
				</li>
				{/db}
			</ul>
		</div>
   </div>
   <div class="gq_music_right"> 
   <div class="gq_music_nh"> 
      <div class="gq_music_right_bt"><h1>旅行篇</h1></div>
	  <div class="gq_music_tw">
		 {db sql="select * from #table_section  where memo ='festival_zhuanti_music_travel'"}
            {$r[data]}
         {/db}
	  </div>
   </div>

   <div class="gq_music_nh"> 
      <div class="gq_music_right_bt"><h1>风景篇</h1></div>
	  <div class="gq_music_tw">
	 {db sql="select * from #table_section  where memo ='festival_zhuanti_music_scenery'"}
            {$r[data]}
         {/db}

	  </div>
   </div>
   <div class="gq_music_nh"> 
      <div class="gq_music_right_bt"><h1>玩乐篇</h1></div>
	  <div class="gq_music_tw">
	 {db sql="select * from #table_section  where memo ='festival_zhuanti_music_play'"}
            {$r[data]}
         {/db}

	  </div>
   </div>
   <div class="gq_music_nh"> 
      <div class="gq_music_right_bt"><h1>宅男宅女篇</h1></div>
	  <div class="gq_music_tw">
	 {db sql="select * from #table_section  where memo ='festival_zhuanti_music_ hometype'"}
            {$r[data]}
         {/db}

	  </div>
   </div>
   </div>
