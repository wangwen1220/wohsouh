<script type="text/javascript">
var nowPage = 'submitted';
var manage_td = '<img src="{IMG_URL}images/edit.gif" title="编辑" alt="编辑" width="16" height="16" class="hand" onclick="contribute.edit({contentid})"/>&nbsp;<img src="{IMG_URL}images/reject.gif" title="撤销" alt="撤销" width="16" height="16" class="hand" onclick="contribute.cancel({contentid})"/>&nbsp;<img src="{IMG_URL}images/delete.gif"title="删除" alt="删除" width="16" height="16" class="hand" onclick="contribute.del(\'{contentid}\');"/>';
var row_template  ='<tr id="row_{contentid}">';
	row_template +='	<td name="title" value="{title}" width="496" class="txt-l"><a href="javascript:;" onclick="contribute.edit({contentid})">{title}</a></td>';
	row_template +='	<td width="75" id="manage_{contentid}" name="manage" value="manage">'+manage_td+'</td>';
	row_template +='	<td width="60">{catname}</td>';
	row_template +='	<td width="120">{created}</td>';
	row_template +='</tr>';
</script>