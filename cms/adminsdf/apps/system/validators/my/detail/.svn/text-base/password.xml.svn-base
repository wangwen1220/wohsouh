<?xml version="1.0" encoding="UTF-8"?>
<root>
	<last_password tips="请输入原密码">
		<required event="submit">请输入原密码</required>
	</last_password>
	<password tips="请输入新密码">
		<required event="submit">请输入新密码</required>
	</password>
	<confirm_password tips="请重复输入新密码">
		<required event="submit">请重复输入新密码</required>
		<eq args="#password">两次输入的密码不一致</eq>
	</confirm_password>
</root>