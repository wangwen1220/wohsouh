.ct_selector {border:1px solid #eee; font-size:12px; height:16px; line-height:16px; padding:2px 0 2px 3px; background:url(bg.gif) no-repeat right 0;cursor:pointer;}
.ct_selector.ct_hover,.ct_selector.ct_focus{border:1px solid #ccc; background-position: right -22px;}
.ct_dropdown {font-size:14px;background-color:#fff;	border:#94C5E5 1px solid;color:#000;position:absolute;cursor:default;list-style:disc;display: none;	z-index:10;}
ul.ct_dropdown, .ct_dropdown li {margin:0;padding:3px 0;}
.ct_dropdown li.ct_item{background-color:#fff;list-style:none;padding-left:16px;border:1px solid #fff; border-width:1px 0;}
.ct_dropdown li.ct_hover{background-color:#F3F7FD;border:1px solid #D2E7EC;cursor:pointer;color:#000; border-width:1px 0;}
.ct_dropdown li.ct_selected{background:url(bg.gif) no-repeat 0 -220px;}
.ct_selector span,.ct_dropdown span{background:url(bg.gif) no-repeat 0 10px;font-size:12px; line-height:17px; height:16px; text-indent:20px; display:block;}
.ct_selector span.picture,.ct_dropdown span.picture{background-position:0 -337px;}
.ct_selector span.link,.ct_dropdown span.link{background-position:0 -99px;}
.ct_selector span.video,.ct_dropdown span.video{background-position:0 -123px;}
.ct_selector span.interview,.ct_dropdown span.interview{background-position:0 -148px;}
.ct_selector span.debate,.ct_dropdown span.debate{background-position:0 -169px;}
.ct_selector span.spider,.ct_dropdown span.spider{background-position:0 -193px;}
.ct_selector span.activity,.ct_dropdown span.activity{background-position:0 -252px;}
.ct_selector span.vote,.ct_dropdown span.vote{background-position:0 -171px;}
.ct_selector span.survey,.ct_dropdown span.survey{background-position:0 -310px;}
.ct_selector span.article,.ct_dropdown span.article{background-position:0 -50px;}
.ct_selector span.special,.ct_dropdown span.special{background-position:0 -280px;}
.ct_selector span.noico, .ct_dropdown span.noico{background:none;text-indent:0px}
.ct_selector span.noico{text-indent:5px}
.ct_selector span.group-pic,.ct_dropdown span.group-pic{background-position:0 -336px;}