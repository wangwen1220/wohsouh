/* common slide*/
.common_roll_slide{}
.common-roll-slide-pages{position:relative; overflow:hidden;}
.common-roll-slide-page{position:relative;}
.common-roll-slide-page-info{position:absolute;padding:0 0 22px 0; bottom:0; width:100%; overflow:hidden;background-color:rgba(0, 0, 0, 0.5); filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr=#80000000, endcolorstr=#80000000, gradientType=1);}
.common-roll-slide-page-info-text{position:relative; padding:5px; color:#fff;}
.common-roll-slide-page-info-text a{color:#fff;}
.common-roll-slide-page-info-text a:hover{color:#fff; text-decoration:underline;}
.common-roll-slide-page-info-text h3{font-size:16px;font-weight:bold;}
.common-roll-slide-page-info-text p{padding-top:5px; line-height:20px;}
.common-roll-slide-nav{padding:2px; position:relative; bottom:18px; font-family:"宋体";}
.common-roll-slide-nav a{display:inline-block; width:18px; height:14px; overflow:hidden; line-height:14px; text-align:center; margin-right:2px; color:#fff;}
.common-roll-slide-nav-links a{background:#3d3d3d;}
.common-roll-slide-nav-links a:hover,
.common-roll-slide-curr{background:#C4101C!important;color:#fff!important;}
.common-roll-slide-nav-right{position:absolute; right:0; font-weight:bold;}
.common-roll-slide-nav-right a:hover {color:#C4101C;}
.common-roll-slide-prev {background:url(../images/wow/slider_nav.png) 4px 2px no-repeat; }
.common-roll-slide-prev:hover {background:url(../images/wow/slider_nav.png) 4px -16px no-repeat; }
.common-roll-slide-next {background:url(../images/wow/slider_nav.png) -10px 2px no-repeat; }
.common-roll-slide-next:hover {background:url(../images/wow/slider_nav.png) -10px -16px no-repeat; }
.common-roll-slide .prev-disabled{color:#555;}
.common-roll-slide .next-disabled{color:#555;}
.common-roll-slide .pagebox_pages_wrap{position:relative; overflow:hidden;}
.common-roll-slide .pagebox_pages_wrap_body{width:9999em;}
.common-roll-slide .pagebox_page{float:left;}
