.music_title {background: url(../images/index_title_bg.png) left 510px; width:185px; height:42px; float:left; margin:0px 0 0 10px;}
.music_channel {float:right; line-height:40px; color:#808080; padding-right:9px;}
.music_channel a {color:#808080;}
.music_channel a:hover {color:#d60921;}

/* left */
.album {width:298px; height:212px; border:1px solid #e5e5e5; float:left; display:inline; overflow:hidden; margin-top:8px;}

.album_content {width:274px; height:93px; margin:4px 12px; float:left;}
.album_cover_bg {background:url(../images/album_cover_bg.png) no-repeat; width:109px; height:93px; float:left; padding:1px 0 0 7px; float:left;}
.album_profile {width:140px;  margin-left:14px; float:right; overflow:hidden;}
.album_intro {height:64px; padding:2px 8px; line-height:22px; color:#808080; clear:both;}

.show {width:298px; height:216px; border:1px solid #e5e5e5; float:left; display:inline; overflow:hidden; margin-top:8px;}
.show_content {padding:2px 8px; clear:both;}
.show_pic {padding:4px;}
.show_pic img {padding:2px; border:1px solid #e5e5e5;}

/* middle */

/* right */
.music_chart_title {width:250px; height:28px; float:left; margin-top:8px; margin-bottom:-8px;}

.vote {width:248px; height:256px; border:1px solid #e5e5e5; float:left; overflow:hidden; margin-top:8px; }
.vote_title {border-bottom:2px solid #e5e5e5; width:240px; margin:2px 4px; height:31px;}
.vote_icon {background:url(../images/index_bg.png) right 586px; width:26px; height:26px; display:inline-block; float:left; margin:2px 5px 0 0;}
.vote_text {width:216px; height:88px; margin:4px auto; color:#8a8a8a; overflow:hidden; line-height:20px;}
.vote_options {width:216px; height:80px; margin:0px auto; overflow:hidden;}
.vote_options  li{ line-height:20px; height:20px;}
.vote_button {width:134px; height:32px; margin:4px auto;}

/* body game */
.game_title {background: url(../images/index_title_bg.png) left 400px; width:201px; height:42px; float:left; margin:0px 0 0 10px;}
.game_channel {float:right; line-height:40px; color:#808080; padding-right:9px;}
.game_channel a {color:#808080;}
.game_channel a:hover {color:#d60921;}

.hot_webgame {width:298px; height:164px; border:1px solid #e5e5e5; float:left; display:inline; overflow:hidden; margin-top:8px;}
.hot_webgame_content {width:282px; height:82px; margin:8px; float:left;}
.hot_webgame_pic {width:111px; height:77px; border:1px solid #e5e5e5; padding:2px; float:left;}
.hot_webgame_text {width:151px; height:77px; padding:3px; float:right;}
.webgame_button_bg {float:left; background:url(../images/game_v_bj.jpg) no-repeat; width:284px; height:20px; padding:6px; margin:1px}
.webgame_button_gray {color:#fff; background:url(../images/index_title_bg.png) left 335px; width:37px; height:20px; display:inline-block; text-align:center; line-height:23px; margin-right:5px;}
.webgame_button_blue {color:#fff; background:url(../images/index_title_bg.png) left 308px; width:37px; height:20px; display:inline-block; text-align:center; line-height:23px; margin-right:5px;}
.webgame_button_orange {color:#fff; background:url(../images/index_title_bg.png) left 278px; width:65px; height:20px; display:inline-block; text-align:center; line-height:23px;}
.webgame_button_gray a {color:#5c5c5c} 
.webgame_button_blue a, .webgame_button_orange a {color:#fff}
.webgame_button_more a {color:#8a8a8a; text-decoration: underline; line-height:23px; }

.flashgame {width:298px;border:1px solid #e5e5e5; height:146px; float:left; display:inline; margin-top:8px;}
.flashgame_list {clear:both;}
.flashgame_list li {float:left; padding:5px 0 0 5px; text-align:center; line-height:30px;}
.flashgame_list li img {padding:2px; background:#FFF; border:1px solid #CCC;}

.mobilegame {width:298px; border:1px solid #e5e5e5; height:207px; float:left; display:inline; margin-top:8px;}
.mobilegame_content {padding:12px 12px 0 12px; clear:both; height:68px; overflow:hidden;font-family: arial;}
.mobilegame_content_pic {width:68px; float:left;}
.mobilegame_content_pic a{width: 60px; height: 60px; background: url('../images/icon.png') no-repeat scroll 0pt 0pt transparent; position: absolute;}
.mobilegame_content_pic img{width: 48px; height: 48px; display: block; margin-top: 6px; margin-left: 6px;}
.mobilegame_content_text { float:right; color:#8a8a8a;line-height: 25px; width: 200px;}
.mobilegame_content_text h1 {font-weight:bold; color:#414141;}
.mobilegame_content_text h4{color: #414141;}
.mobilegame_content_text_intro{ margin-top: 5px; float: left;}
.mobilegame_content_text_intro a {width: 98px; height: 26px; background:url('../images/down.png') no-repeat scroll 0pt 0pt transparent; text-indent: -999em; float: left;}
.mobilegame_content_text h1 a {color:#414141;}
.mobilegame_content_text span{float: left; margin-top: 5px; margin-left: 5px;}

/* middle */

/* right */
.game_topic {background:url(../images/index_title_bg.png) right 476px; width:250px; height:236px; display:inline-block; float:left;}
