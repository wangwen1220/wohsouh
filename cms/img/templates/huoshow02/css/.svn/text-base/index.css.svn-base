.game_beauty_text {width:100%; position:absolute; bottom:0; background:rgba(0, 0, 0, 0.5); filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr=#80000000, endcolorstr=#80000000, gradientType=1);}
.game_beauty_text a{color:#fff; line-height:20px;}

