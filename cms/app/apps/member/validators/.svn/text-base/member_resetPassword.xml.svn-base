<?xml version="1.0" encoding="UTF-8"?>
<root box="errorbox">
	<password>
		<required>请填写密码</required>
		<min args="6">密码至少6位</min>
		<max args="20">密码最多20位</max>
		<password>格式不正确</password>
	</password>
	<password_check>
		<required>请再次填写密码</required>
		<password>格式不正确</password>
		<eq args="#password">新密码不一致</eq>
	</password_check>
</root>