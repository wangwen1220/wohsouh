{template "community","myshow_header"}
<div id="wrapper" class='pin-details fn-clear'>
	<div id="aside">
		<div class="album">
			<h5>出自专辑</h5>
			<h3>{$album_datas[0][album_name]}</h3>
			<span class='pin-position'><em>{$first_r}</em>/{$total_r}</span>
			<a class="link" href='/zj-{$share_info[albumid]}.html'>
			{loop $thumb_data $r}
			<img src='{$r[thumb]}' width='61' height='61' />
			{/loop}
			</a>
			<a class='btn' href='#'>取消关注</a>
		</div>
		<div class='domain-pins'>
			<h3>同出自</h3>
			<div class='album-bar'>
				{loop $same_data $r}
				<a href='#'><span class='text'>{$r[album_name]}</span><span class='stats'>{$r[file_count]}</span></a>
				{/loop}
			</div>
		</div>
	</div>
	<div id="main" class='goods'>
		<div class='pin-show'>

			<div class='pinner fn-clear'>
				<a class="pinner-photo" href="/wangwen1220/"><img src="{$share_info[owner_avatar]}" width='52' height='52' /></a>
				<p class="pinner-date">{$share_info[input_time]}</p>
				<p class="pinner-name"><a href="/wangwen1220/">{$share_info[nickname]}</a></p>
				<p class="pinner-from">首发自专辑：<a href="/wangwen1220/">{$share_info[album_name]}</a></p>
			</div>
			<div class='pin-goods'>
				<a class='goods-img' href='#'><img src='{$share_info[file_path]}' alt='' /></a>
				<h2 class='goods-title'>{$share_info[file_name]}<span class='from taobao'></span></h2>
				<a class='buy-btn' href='#'><span class='price'>￥{$share_info[price]}</span></a>
				<div class='pin-actions'>
					<ul class='pin-buttons'>
						<li class="btn repin"><a href="#">转采</a></li>
						<li class="btn like"><a href="#">喜欢 <span>250</span></a></li>
					</ul>
				</div>
				<div class="tags"><span class='tag-title'>标签：</span><a class="tag-type" href='#'>图片</a> | <a class='tag' href='#'>#风景</a> <a class='tag' href='#'>#摄影</a> <a class='tag' href='#'>#美女</a> <a class='tag' href='#'>#尤物</a></div>
			</div>
			<p class="desc">{$share_info[description]}</p>
