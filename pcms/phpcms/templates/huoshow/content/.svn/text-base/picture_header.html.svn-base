<script>
	$(function(){
		window.clearKeyword = function(){
			var search_wd = $.trim($("#q").val());
			if(search_wd=='请输入关键字'){
				$("#q").val('');
			}
			if(search_wd==''){
				$("#q").val('请输入关键字');
			}
		}
		window.checkSForm = function()
		{
			var search_wd = $.trim($("#q").val());
			if(search_wd=='请输入关键字' || search_wd==''){
				alert('请输入关键字');
				return false;
			}
		}
	})
</script>
<script>
  function sTo(o) {
    var id = o.href.split('#')[1];
    var t = document.getElementById(id);
    var y = 0;
    if(t){
      y = t.offsetTop - 36;
    }
    scrollTo(0, y);
  }
</script>
