{template "content","game_header"}
<div id='wrapper'>
<!-- Top Content -->
<div class='ui_content top_content top_content_mobile fn_clear'>
	<div class="ui_content_left fn_left">
		<div class="pic_slide_wrapper">
			<!-- slide start -->
			<div class="pic_slide">
				{pc:content  action="position" posid="214" catid="$catid" thumb="1" order="listorder DESC" num="5"}
				<ul>
					{loop $data $r}
					<li>
						<a href="{str_replace('http://www.huoshow.com/games/','http://game.huoshow.com/',$r[url])}" target='_blank'><img src="{thumb($r[thumb],290,250)}" alt="{$r[title]}" width='290' height='250' /></a>
						<h3>{str_cut($r[title],50,'')}</h3>
					</li>
					{/loop}
				</ul>
				<ol>
					{loop $data $r}
					<li class='num'><span>{$n}</span></li>
					{/loop}
					<li class='next'><span>Next</span></li>
					<li class='prev'><span>Prev</span></li>
				</ol>
				{/pc}
			</div>
		</div>
		<!-- slide end -->
	</div>

	<div class='ui_content_center fn_left'>
		{pc:content  action="position" posid="222" catid="$catid" order="listorder DESC" num="1"}
		{loop $data $r}
		<h2 class='ui_title'><a href="{str_replace('http://www.huoshow.com/games/','http://game.huoshow.com/',$r[url])}" target='_blank' title='{$r[title]}'>{str_cut($r[title],80,'')}</a></h2>
		{/loop}
		{/pc}
		{pc:content  action="position" posid="215" catid="$catid" order="listorder DESC" num="4"}
		<ul class='ui_title_list'>

				<li>{loop $data $r}
					[<a href="{str_replace('http://www.huoshow.com/games/','http://game.huoshow.com/',$r[url])}" target='_blank' title='{$r[title]}'>{str_cut($r[title],40,'')}</a>]
					{if $n%2==0}<br>{/if}
					{/loop}
				</li>

		</ul>
		{/pc}
		{pc:content  action="position" posid="222" catid="$catid" order="listorder DESC" num="2"}
		{loop $data $r}
		{if $n!=1}
		<h2 class='ui_title'><a href="{str_replace('http://www.huoshow.com/games/','http://game.huoshow.com/',$r[url])}" target='_blank' title='{$r[title]}'>{str_cut($r[title],80,'')}</a></h2>
		{/if}
		{/loop}
		{/pc}
		{pc:content  action="lists" catid="96" order="updatetime DESC" num="5"}
		<ul class='ui_news_list'>
			{loop $data $r}
			<li><a href="{str_replace('http://www.huoshow.com/games/','http://game.huoshow.com/',$r[url])}" target='_blank' title='{$r[title]}'>[{$CATEGORYS[$r[catid]][catname]}] {str_cut($r[title],64,'')}</a></li>
			{/loop}
		</ul>
		{/pc}
	</div>
</div>
<!-- Top Content End -->

<!-- 视频攻略 -->
<div class='ui_box ui_box_video'>
