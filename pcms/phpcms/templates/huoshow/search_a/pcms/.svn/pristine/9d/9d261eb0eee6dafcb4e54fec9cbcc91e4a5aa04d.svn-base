{template "content","game_header"}
<!-- Top AD -->
<div class='ad ad_online'>
	<ul class='ad_list'>
	{pc:content  action="position" posid="226" order="listorder DESC" num="6" }
   	{loop $data $r}
		<li {if $n==1}class='first'{/if}><a target="_blank" href="{$r[url]}" title="{$r[title]}"><img src="{thumb($r[thumb],153,115)}" alt="{$r[title]}" width='153' height='115' /></a></li>
	{/loop}
	{/pc}
	</ul>
</div>
<!-- Top AD End -->

<!-- Top Content -->
<div class='ui_content top_content fn_clear'>
