.top_video_text{ width:138px;text-align:center; line-height:22px;}
.body_right { width:250px; height:auto; float:right; }
/* hot movie */
.hot_movie {width:250px; height:382px; overflow:hidden; background:url(../images/movie_hot_movie.jpg);}
.hot_movie_cate { padding-right:10px; text-align:right; }
.hot_movie_cate a { color:#f3dc5b; line-height:50px; width:41px; height:55px; display:inline-block; text-align:center;}
.movie_cate_def, .hot_movie_cate a:hover {background: url(../images/hover_bj.jpg) left 220px; color:#363447!important; position:relative; top:-2px;}

.hot-movie-body {padding:0 0 0 16px;width:226px; height:296px; overflow:hidden; position:relative;}
.hot_movie .pagebox_pages_wrap{ width:226px; height:296px; overflow:hidden;}
.hot_movie .pagebox_pages_wrap_body{width:9999em;}
.hot_movie_ul{overflow:hidden;width:230px;height:296px;}
