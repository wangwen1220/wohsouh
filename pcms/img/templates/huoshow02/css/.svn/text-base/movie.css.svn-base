.hot_movie_ul li a { display:block;}
.hot_movie_ul li img{ width:105px; height:142px;}
.hot_movie_block { width:100%; position:absolute; background: #363636; filter:alpha(opacity=80); opacity:.80; overflow:hidden; bottom:0;}
.hot_movie_text { display:block; color:#fff; padding: 5px;}

