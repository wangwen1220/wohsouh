@charset "utf-8";
/* CSS Document */
.body_bg { background-color: #fff; background-image: url(../images/star_body_bg.jpg); background-repeat: repeat-x;}
/* nav */
.nav_body {margin:0 auto; width:960px; height:76px;}
.nav_bg {background:url(../images/star_logo.jpg) no-repeat; margin: 0 auto; height:75px; position:relative; left:-192px; top:4px;}
.channel_logo a {width: 200px; height: 72px; display: block; text-indent: -999em; margin: 2px; float: left; position:relative; left:192px;}

#sub_channel { width:740px; height:35px; float:right; display:inline; position:relative; left:220px;}
#sub_channel ul { width:598px;height:35px; display:inline;  overflow:hidden; margin-top:40px; float:right;}
#sub_channel ul li { float:left;}
#sub_channel ul li a {width:94px; height:37px;  color:#FFF; font-size:16px;  background:url(../images/b1.jpg) right top;text-align:center;display:inline-block; line-height:35px; margin-left:1px;}
#sub_channel ul li a:hover{width:94px; height:37px;color:#ffd524;   font-size:16px; background:url(../images/b1.jpg) left top; text-align:center; display:inline-block;line-height:35px;}

/* footer */
#footer_bg {background-color:#1e1e1e; margin-top:20px;}
#footer {width:960px; height:100px; margin:0 auto; background-color:#1e1e1e;}
.footer_txt {width:700px; color:#b6b6b6; line-height:16px; padding-top:26px;}
.footer_txt a {color:#b6b6b6; }
.footer_logo {background:url(../images/hover_bj.jpg) left 130px; width:90px; height:60px; float:left; margin-right:15px;}

/* search */
.search {width:960px; height:35px; margin:0 auto; line-height:30px; vertical-align:middle; margin-bottom:12px}
.search_text {float:left; margin-left:10px;width:510px; height:35px; line-height:40px;}
.search_text a {color:#1e1e1e;}
.search_input {float:left; margin:8px 0 0 130px; }
.search_dropdown {float:left; margin:8px 0 0 5px; height:20px; }
.search_button {float:left; margin:9px 0 0 5px; background:url(../images/bg.jpg) left 666px; width:58px; height:18px;}
.search_button a{width:58px; margin-left:15px; line-height:18px;}
.input, .select{color:#000}

