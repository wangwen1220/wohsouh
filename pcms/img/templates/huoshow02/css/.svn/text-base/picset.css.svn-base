.meitu_des{margin:0 auto; width:960px;     border: 1px solid #D2D2D2; margin-top:8px}
.meitu_des li{ float:left; width:140px; margin:12px 10px; _margin:12px 9px; font-size:14px; }
.meitu_des li img{margin-bottom:8px} 
.meitu_des h2{background:url(../images/ass.jpg) no-repeat left center; color:#414141; line-height:26px}

a[class ^= "click_skip"]{display:block; width:22px; height:42px;   position:relative; top:64px}
* html .click_skip_left, * html .click_skip_right{ display:block; width:22px; height:42px;   position:relative; top:64px} 
.click_skip_left{left:8px; float:left}
.click_skip_right{ right:8px; float:right}


.guide{ width:60px; border:1px solid #e8e8e8; height:188px;  position:fixed; top:200px; right:120px; background:#FFF; z-index:99999;}
.guide ul li a{display:block; height:26px; line-height:26px; text-align:center; border-bottom:1px solid #e8e8e8;}

* html .guide{position:absolute; left:expression(eval(document.documentElement.scrollLeft+10));  top:expression(eval(document.documentElement.scrollTop+10));}


.slide {position:relative; width:700px; height:324px; overflow:hidden;}
#pbox3 .pagebox_pages_wrap{width:700px; height:324px; overflow:hidden;}
#pbox3 .pagebox_pages_wrap_body{width:9999em; height:324px;}
#pbox3 .pagebox_page{float:left; width:700px; height:324px; overflow:hidden;}
.slide .slide-page{height:324px; position:relative; overflow:hidden;}
.slide .slide-page-info{ position:absolute; color:#fff; width:100%; overflow:hidden; bottom:0; left:0; background-color:rgba(0, 0, 0, 0.5); filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr=#80000000, endcolorstr=#80000000, gradientType=1); }
.slide .slide-page-info-text{position:relative; padding:10px 10px 45px;}
.slide .slide-page-info-text h3{font-size:26px; font-family:"黑体"; width:315px; float:left;}
.slide .slide-page-info-text p{ width:315px; line-height:28px; float:right; margin-right:10px;}
.slide .slide-page-info-text a { color:#fff;}
.slide .slide-page-info-text a:hover{ color:#fff; text-decoration:underline;}
.slide .slide-links-wrap{width:100%;height:30px; position:absolute; bottom:8px; text-align:center; color:#fff;}
.slide .slide-links-wrap a{ background-image:url(../images/movie_slide_nav.png); vertical-align:middle; display:inline-block; overflow:hidden; margin-right:5px;}
.slide .slide-links a{ background-position:left -186px; width:15px; height:15px; }
.slide .slide-links .slide-current{ background-position:left -81px; }
.slide .slide-prev{ background-position:left top; width:20px; height:30px; }
.slide .prev-disabled{ background-position:left -105px;}
.slide .slide-next{ background-position:left -40px; width:20px; height:30px; }
.slide .next-disabled{ background-position:left -145px;}
.slide_shadow {background:url(../images/bg.jpg) left 324px; width:0; height:17px;}

/* 影视图片 */
.Subpage_pic{float: left; height: auto; width: 960px;}
.Subpage_pic_01 { background: none repeat scroll 0 0 #F9F9F9; border: 1px solid #DEDCDD; display: inline; float: left; width: 958px;}
.Subpage_pic_list{ border: 1px solid #FFFFFF; float: left; width: 956px;}
.Subpage_pic_bt{ width:956px; height:37px; background:url(../images/ass.jpg) no-repeat left center;}
.Subpage_pic ul { list-style: none outside none; overflow: hidden; padding: 10px 0 0 0; width: 930px; margin: 0 auto; }
.Subpage_pic ul li { background: url(../images/bg_border.png) repeat-x scroll 0 100% transparent; display: inline; float: left; height: 150px; margin: 0 0 5px; overflow: hidden; padding: 5px 12px 10px; width: 160px; }
.Subpage_pic ul li a { overflow: hidden; text-align: center;}
.Subpage_pic ul li img { border: 1px solid #D1D1D1; display: block; height: 118px; padding: 2px; width: 158px; }
.Subpage_pic ul li span { display: block; line-height:25px; height:20px;}

