.fnews{
	position:relative;
	clear:both;
	width:540px;
	height:260px;	
	background:url(images/fnews_bg.gif) -1080px top repeat-y;
	overflow:hidden;
	}
.fnews .fnews_top{
	height:10px;
	background:url(images/fnews_bg.gif) left top no-repeat;
	}
.fnews .fnews_pic{
	height:220px;
	padding:10px;
	padding-top:0px;
	}
.fnews .fnews_pic p{
	font-weight:600;
	}
.fnews .fnews_foot{
	position:absolute;
	width:540px;
	height:14px;
	padding-bottom:6px;
	background:url(images/fnews_bg.gif) -540px bottom no-repeat;
	left:0px;
	top:240px;
	}
.fnews .fnews_foot ul{
	position:absolute;
	width:140px;
	height:14px;
	left:400px;
	top:-2px;	
	}
.fnews .fnews_foot ul li{
	float:left;
	padding-right:8px;
	}
.fnews_foot .bt{
	height:14px;
	width:8px;
	background:url(images/dot.gif) left center no-repeat;
	}
.fnews_foot .bt_c{
	height:14px;
	width:8px;
	background:url(images/dot_c.gif) left center no-repeat;
	}
.title_01{
