.slides { width:640px; margin:5px 0px 10px 0px;}
#top2banner{ position:relative; width:640px; height:250px; background:#333333; margin:0px 0px 10px 0px; float:right;}
.top2banner-nav a:hover, .top2banner-nav .tab-on{ background:#f90;}
.top2banner-nav a{ background:#aaa; padding:0 5px; line-height:18px; display:inline-block; color:#fff;}
.top2banner-nav{ text-align:right; right:10px; top:228px; position:absolute;}
.top2banner-item{ position:absolute; width:640px; height:250px; overflow:hidden; display:none;}
.top2banner-item .thumb{ width:640px; height:250px;}
.top2banner-item-title{ height:24px; line-height:24px; top:-25px; position:relative; bottom:0; padding:0 2px; background:#000000;}
.top2banner-item-title a{ z-index:1000; color:#fff; position:absolute; padding-left:1em;}
.top2banner-item-title-bg{ background:#000; opacity:0.5; filter:alpha(opacity=50); display:block;}
