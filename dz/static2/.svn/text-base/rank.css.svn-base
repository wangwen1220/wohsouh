@charset "utf-8";
/* CSS Document */

body { font-family:"宋体", arial; margin:0; padding:0; background: #fff; font-size:12px; color:#909090; ; background-repeat: no-repeat; background-position:center top; background-color:#fff; font-size:12px; }
div, form, img, ul, ol, li, dl, dt, dd, p { margin: 0; padding: 0; border: 0; }
li, dl { list-style-type:none; }
h1, h2, h3, h4, h5, h6 { margin:0; padding:0; font-size:14px; color: #000; }
a { color:#909090; text-decoration:none; }
a:hover { color: #FF0000; text-decoration:underline; }
input { vertical-align:middle; }
.clear {clear:both; }

