<div class="blr" style="width:430px;background:url(/static2/images/gifts/liwu05.jpg)">
  <h3 id="layer_reginfo_t" class="flb">
    <em id="returnmessage4">确认出售 $typeName ?</em>
    <span>
      <a href="javascript:;" class="flbc" onclick="hideWindow('register')" title="关闭">关闭</a></span>
  </h3>

  <!--{if $typeName=='全部'}-->
  <div id="tangchuang2s">
    <div class="quedmc"><img width="280" height="50" src="/static2/images/gifts/liwu2.png"></div>
    <p class="quedwz">总计收入：<span class="text8">{$coinName} {$sumPrice}</span></p>
    <p class="qiedingbg"><a href="javascript:;" onclick="sellGift('all')">
      <img width="86" height="24" alt="确定"  src="/static2/images/gifts/liwu4.png"></a>　　　
    <a href="javascript:;" onclick="hideWindow('register')"><img width="86" height="24" alt="取消" src="/static2/images/gifts/liwu5.png"></a></p>
  </div>
  <!--{else}-->
  <div class="tangchuang2s">
    <table width="100%" height="146" border="0" cellpadding="0" cellspacing="0">

      <tr>
        <td width="38%" height="135" align="left" valign="top"><table width="100%" height="139" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td height="33" colspan="3" align="center" valign="middle">
                <div class="liwubjing"><img src="/static2/images/gift/$value[image]" alt="{$value['name']}" width="48" height="48" /></div></td>
            </tr>
            <tr>
              <td width="32%" align="right" valign="middle">
                <input type="image" name="imageField2" id="imageField2"
                onclick="e_nums('$value['giftid']')" src="/static2/images/gifts/liwu08.jpg" />
              </td>
              <td width="34%" align="center" valign="middle">
                  <input name="textarea" type="text" class="shuzikuangzi" id="sellnums_$value['giftid']" value="$value['num']" size="4" maxlength="4" onkeypress="validateInt(this);" onkeyup="sellNumChange(this);" onpaste="return false;" style="ime-mode: disabled;" />
              </td>
              <td width="34%" align="left" valign="middle">
                <input type="image" name="imageField" onclick="a_nums('$value['giftid']')" id="imageField" src="/static2/images/gifts/liwu09.jpg" />
              </td>
            </tr>
            <tr>
              <td colspan="3" align="center" valign="middle">请输入卖出数量(1~$value['num'])</td>
            </tr>
        </table></td>
        <td width="6%">&nbsp;</td>
        <td width="56%" align="left" valign="top"><table width="100%" height="143" border="0" cellpadding="0" cellspacing="0">
            <tr>
              <td height="35" valign="top" class="liwudamingz">$typeName</td>
            </tr>
            <tr>
              <td height="35">选择币种：  <select name="cointype" id="cointype" onchange="changeCointype(this);">

                  <option value="2">火币</option>
                  <option value="1">秀币</option>
              </select></td>
            </tr>
            <tr>
              <td>卖出单价：  <span class="text8"><span id="coinname1">火币</span>  <em id="danprice">$value['sellprice']</em></span></td>
            </tr>
            <tr>
              <td height="39">总计收入：  <span class="text8"><span id="coinname2">火币</span>  <em id="endprice">$value['totalprice']</em></span></td>
            </tr>
            <tr>
              <td height="12">&nbsp;</td>
            </tr>
            <tr>
              <td><label><a href="javascript:;" onclick="sellGift('$value['giftid']');" ><img src="/static2/images/gifts/liwu4.png" width="86" height="24" /></a> 
                  <a href="javascript:;" onclick="hideWindow('register')" ><img src="/static2/images/gifts/liwu5.png" width="86" height="24" /></a></label></td>
            </tr>
        </table></td>
      </tr>
    </table>
  </div>
  <!--{/if}-->
  <script>
    var cointype=2;//定义默认币种为秀币，1为火币
    (function($){
      window.sellGift = function (id) {
        if(id=='all'){
          var url = 'home.php?mod=space&do=gifts&act=sell&cointype={$cointype}&ajax=t&giftid='+id+'&jsoncallback=?';
        }else{
          var nums = $("#sellnums_"+id).val();
          var url = 'home.php?mod=space&do=gifts&act=sell&cointype='+cointype+'&ajax=t&sellnums='+nums+'&giftid='+id+'&jsoncallback=?';
        }		
        $.getJSON(url, function(data) {
          if(data=='1'){
            alert('出售成功');
            window.location.reload();
          }else{
            alert('出售失败');
          }
        });
      };
    })(jQuery);
  </script>
  <!--{if $typeName!='全部'}-->
  <script>
    (function($){
      var danjia = {$value['sellprice']};
      window.changeCointype = function (e) {
        cointype = e.value;
        var nums= $("#sellnums_{$value['giftid']}").val();
        if(cointype=='1'){
          $("#coinname1").text('秀币');
          $("#coinname2").text('秀币');
          danjia = {$value['sellprice']};
          $("#danprice").text('{$value[sellprice]}');
          var endprice = Math.floor(nums*danjia);
          $("#endprice").html(endprice);
        }else
        {
          $("#coinname1").text('火币');
          $("#coinname2").text('火币');
          danjia = {$value['sellprice']};
          $("#danprice").text('{$value[sellprice]}');
          var endprice = Math.floor(nums*danjia);
          $("#endprice").html(endprice);
        }
      };


      window.e_nums = function(id) {
        var nums= $("#sellnums_"+id).val();
        var endnums = nums-1;
        var endprice = Math.floor(endnums*danjia);
        if(endnums>0){
          $("#sellnums_"+id).val(endnums);
          $("#endprice").html(endprice);
        }
      };


      window.a_nums = function(id) {
        var nums= $("#sellnums_"+id).val();
        var endnums = parseInt(nums)+1;
        var totalnum = $value['num'];
        var endprice = Math.floor(endnums*danjia);
        if(endnums<=totalnum){
          $("#sellnums_"+id).val(endnums);
          $("#endprice").html(endprice);
        }
      };

      window.validateInt = function (node) {
        if(validateInt.caller){
          var e = validateInt.caller.arguments[0] || event;
          var key = e.which || e.keyCode;
          if(!(key == 8 || key == 37 || key == 39 || key == 35 || key == 36 || /\d+/.test(String.fromCharCode(key)))){
            preventDefault(e);
          }
        }
      };

      window.sellNumChange = function (node) {
        var totalnum = parseInt("$value['num']");
        var value = parseInt(node.value);
        if(value===0){
          node.value = 1;
        }
        if(value>totalnum){
          node.value = totalnum;
        }

        var endnums = $("#sellnums_$value['giftid']").val();
        var endprice = Math.floor(endnums*danjia);
        $("#endprice").html(endprice);
      };

    })(jQuery);
  </script>
  <!--{/if}-->
</div>
